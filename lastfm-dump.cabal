name:                lastfm-dump
version:             0.1.0.0
synopsis:            Dump your scrobbling data from Last.fm
-- description:         
homepage:            http://github.com/jliuhtonen/lastfm-dump
license:             MIT
license-file:        LICENSE
author:              Janne Liuhtonen
maintainer:          janne.liuhtonen@iki.fi
-- copyright:           
category:            Web
build-type:          Simple
-- extra-source-files:  
cabal-version:       >=1.10

executable lastfm-dump
  main-is: Main.hs
  -- other-modules:       
  other-extensions:    OverloadedStrings, DeriveGeneric
  build-depends:       base == 4.8.1.0, aeson == 0.10.0.0, text == 1.2.1.3, time == 1.5.0.1, http-conduit == 2.1.8, bytestring == 0.10.6.0, lens == 4.13, lens-aeson == 1.0.0.5, mongoDB == 2.0.9, bson == 0.3.2, transformers == 0.4.2.0, binary == 0.7.6.1, mtl == 2.2.1
  hs-source-dirs:      src/
  default-language:    Haskell2010
