-- Initial lastfm-dump.cabal generated by cabal init.  For further 
-- documentation, see http://haskell.org/cabal/users-guide/

name:                lastfm-dump
version:             0.1.0.0
synopsis:            Dump your scrobbling data from Last.fm
-- description:         
homepage:            http://github.com/jliuhtonen/lastfm-dump
license:             MIT
license-file:        LICENSE
author:              Janne Liuhtonen
maintainer:          janne.liuhtonen@iki.fi
-- copyright:           
category:            Web
build-type:          Simple
-- extra-source-files:  
cabal-version:       >=1.10

executable lastfm-dump
  main-is: Main.hs
  -- other-modules:       
  other-extensions:    OverloadedStrings, DeriveGeneric
  build-depends:       base == 4.10.1.0, aeson == 1.2.4.0, text == 1.2.3.0, time == 1.8.0.2, http-conduit == 2.3.1, bytestring == 0.10.8.2, lens == 4.16.1, lens-aeson == 1.0.2, mongoDB == 2.3.0.5, bson == 0.3.2.6, transformers == 0.5.2.0, binary == 0.8.5.1, mtl == 2.2.2, dotenv == 0.5.2.5, network == 2.6.3.5
  hs-source-dirs:      src/
  default-language:    Haskell2010
